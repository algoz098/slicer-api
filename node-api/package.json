{
  "name": "node-api",
  "description": "an slicer api made in nodejs",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">=22.0.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "other",
    "framework": "koa",
    "transports": [
      "rest"
    ],
    "schema": "typebox"
  },
  "type": "commonjs",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "prettier \"**/*.{ts,tsx,js,json,md}\" --write",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "mocha": "cross-env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --require ts-node/register --extensions ts test/**/*.test.ts",
    "test:info": "cross-env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --require ts-node/register --extensions ts test/services/files/info/info.test.ts",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.34",
    "@feathersjs/authentication": "^5.0.34",
    "@feathersjs/authentication-client": "^5.0.34",
    "@feathersjs/configuration": "^5.0.34",
    "@feathersjs/errors": "^5.0.34",
    "@feathersjs/feathers": "^5.0.34",
    "@feathersjs/koa": "^5.0.34",
    "@feathersjs/schema": "^5.0.34",
    "@feathersjs/transport-commons": "^5.0.34",
    "@feathersjs/typebox": "^5.0.34",
    "@koa/router": "^14.0.0",
    "@types/yauzl": "^2.10.3",
    "koa-body": "^6.0.1",
    "winston": "^3.17.0",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.34",
    "@feathersjs/rest-client": "^5.0.34",
    "@types/koa__router": "^12.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "axios": "^1.11.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "mocha": "^11.7.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
